type Query {
    ads: [Ads]
    ad(id: Int!): Ads
}

type Ads {
    id: Int!
    description: String
    address: String
    phone: String
    carModel: CarModel
    user: User
}

type User {
    id: Int!
    username: String
    password: String
    type: String
}

type CarModel {
    id: Int!
    make: String
    model: String
    year: String
    kilometers: Int
    displacement: Int
    hp: Int
}

